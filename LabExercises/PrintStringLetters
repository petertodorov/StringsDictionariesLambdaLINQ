using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
class PrintStringLetters
{
    static void Main(string[] args)
    {
        string str = "softUni";

        for (int i = 0; i < str.Length; i++)
        {
            Console.WriteLine("str[{0}] -> {1}", i, str[i]);
        }
    }
}

